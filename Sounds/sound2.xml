<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

	<fx>
 
		<!-- External Sounds -->
 
		<!-- Engines-->
 
		<engine>
			<name>Intake Fans</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-intake.wav</path>
			<condition>
				<not>
					<or>
						<property>/sim/current-view/internal</property>
						<property>/engines/engine/out-of-fuel</property>
					</or>
				</not>
			</condition>
			<position>
				<x>5.5</x>
				<y>0.0</y>
				<z>0.0</z>
			</position>
			<orientation>
				<x>-90.0</x>
				<y>0</y>
				<z>0</z>
				<inner-angle>110.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.4</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine[0]/n2</property>
				<factor>0.08</factor>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.05</factor>
				<min>0.0</min>
				<max>1.0</max>
			</pitch>
		</engine>
  
		<engine>
			<name>APU Exhaust</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/wing-vents.wav</path>
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<property>/sim/current-view/internal</property>
					<property>/engines/engine/out-of-fuel</property>
				</not>
			</condition>
			<position>
				<x>2.0</x>
				<y>0.0</y>
				<z>0.0</z>
			</position>
			<orientation>
				<x>90.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>80.0</inner-angle>
				<outer-angle>110.0</outer-angle>
				<outer-gain>0.6</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine[2]/n2</property>
				<factor>0.03</factor>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>engines/engine[2]/n2</property>
				<factor>0.2</factor>
				<min>0.0</min>
				<max>1.0</max>
			</pitch>
		</engine>
  
		<engine>
			<name>Engine Exhaust</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-exhaust.wav</path>
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<or>
						<property>/sim/current-view/internal</property>
						<property>/engines/engine/out-of-fuel</property>
						<property>voodoomaster/migraine-mode</property>
					</or>
				</not>
			</condition>
			<position>
				<x>6.0</x>
				<y>0.0</y>
				<z>0.0</z>
			</position>
			<orientation>
				<x>90.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>80.0</inner-angle>
				<outer-angle>110.0</outer-angle>
				<outer-gain>0.5</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine/n2</property>
				<factor>0.08</factor>
				<min>0.4</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.005</factor>
				<min>0</min>
				<max>0.7</max>
			</pitch>
		</engine>
  
		<engine>
			<name>Engine Thrust LowFreq</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-lowfreq.wav</path>
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<property>/sim/current-view/internal</property>
					<property>/engines/engine/out-of-fuel</property>
				</not>
			</condition>
			<position>
				<x>10.0</x>
				<y>0.0</y>
				<z>0.0</z>
			</position>
			<orientation>
				<x>90.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>0.0</inner-angle>
				<outer-angle>160.0</outer-angle>
				<outer-gain>0.5</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.0001</factor>
				<offset>0.25</offset>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
		</engine>
  
		<!-- Afterburner -->
  
		<engine>
			<name>Reheat</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/reheat2.wav</path>
			<condition>
				<and>
					<property>engines/engine/augmentation</property>
					<not>
						<or>
							<property>/sim/current-view/internal</property>
							<property>/engines/engine/out-of-fuel</property>
							<property>voodoomaster/migraine-mode</property>
						</or>
					</not>
				</and>
			</condition>
			<position>
				<x>6.0</x>
				<y>0.0</y>
				<z>0.0</z>
			</position>
			<orientation>
				<x>90.0</x>
				<y>0.0</y>
				<z>0.0</z>
				<inner-angle>60.0</inner-angle>
				<outer-angle>160.0</outer-angle>
				<outer-gain>0.75</outer-gain>
			</orientation>
			<volume>
				<offset>1</offset>
				<min>0.3</min>
				<max>1.0</max>
			</volume>
		</engine>
  
