		<click>
			<name>Parking brakes up</name>
			<path>Aircraft/Tornado-J/Sounds/Samples/slide.wav</path>
			<condition>
				<not>
				   <property>controls/gear/brake-parking</property>
				</not>
			</condition>
			<volume>
				<factor>1.0</factor>
				<max>1.0</max>
