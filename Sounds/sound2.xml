		<!-- old sounds setup from A-4 & AlMurSi tunning -->

		<rumble>
			<name>rumble</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/rumble.wav</path>
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<type>lin</type>
				<factor>.012</factor>
				<min>0</min>
				<max>1</max>
			</volume>
		</rumble>
		<gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/gear.wav</path>
			<property>/gear/gear[0]/position-norm</property>
			<pitch>
				<internal>dt_play</internal>
				<offset>1.0</offset>
				<factor>-0.001</factor>
			</pitch>
		</gear>
		<gear>
			<name>gear-lock</name>
			<path>Aircraft/Tornado-J/Sounds/Samples/gear-lck.wav</path>
			<condition>
				<equals>
					<property>/gear/gear[0]/position-norm</property>
					<value>0</value>
				</equals>
			</condition>
		</gear>
		<gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/gear.wav</path>
			<property>/gear/gear[1]/position-norm</property>
			<pitch>
				<internal>dt_play</internal>
