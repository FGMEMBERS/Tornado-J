				<z>0.0</z>
			</position>
			<volume>
				<offset>0.25</offset>
			</volume>
		</ignition>
		<-->
  
		<!-- Cockpit Environment -->
  
		<cockpit>
			<name>Cockpit Atmos</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/cockpit.wav</path>
			<condition>
				<property>/sim/current-view/internal</property>
			</condition>
			<volume>
				<offset>1</offset>
				<min>0.2</min>
				<max>1.0</max>
			</volume>
		</cockpit>
  
		<thrust>
			<name>Internal Engine Thrust LowFreq</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-lowfreq.wav</path>
			<condition>
				<property>/sim/current-view/internal</property>
				<not>
					<property>/engines/engine/out-of-fuel</property>
				</not>
			</condition>
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.00005</factor>
				<offset>0.4</offset>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
		</thrust>
  
		<engine>
			<name>Intake Fans Internal</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-intake.wav</path>
			<condition>
				<property>/sim/current-view/internal</property>
			</condition>
			<volume>
				<property>/controls/canopy/position-norm</property>
				<factor>1.0</factor>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.005</factor>
				<min>0</min>
				<max>0.7</max>
			</pitch>
		</engine>
  
		<engine>
			<name>Exhaust Internal</name>
			<mode>looped</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/engine-exhaust.wav</path>
			<condition>
				<property>/sim/current-view/internal</property>
			</condition>
			<volume>
				<property>/controls/canopy/position-norm</property>
				<factor>1.0</factor>
				<min>0.0</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.005</factor>
				<min>0</min>
				<max>0.7</max>
			</pitch>
		</engine>
  
