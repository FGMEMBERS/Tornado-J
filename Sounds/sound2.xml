			<volume>
				<offset>0.25</offset>
				<max>0.9</max>
			</volume>
		</speedbrake>
		<slat>
			<name>slats</name>
			<mode>in-transit</mode>
			<path>Aircraft/Tornado-J/Sounds/Samples/flaps.wav</path>
